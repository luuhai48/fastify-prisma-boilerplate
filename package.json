{
  "name": "fastify-prisma-boilerplate",
  "version": "0.1.0",
  "main": "dist/index.js",
  "private": true,
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/redis": "^6.1.0",
    "@fastify/sensible": "^5.2.0",
    "@prisma/client": "^4.8.1",
    "close-with-grace": "^1.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.4.0",
    "prisma": "^4.8.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "json-schema-to-ts": "^2.6.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "typescript-module-alias": "^2.2.9"
  },
  "scripts": {
    "dev": "nodemon --exec typescript-module-alias ./src/index.ts",
    "build": "typescript-module-alias-build ./src",
    "format": "yarn lint:write && yarn format:write",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint . --ext .ts --fix",
    "migrate": "prisma migrate dev"
  }
}
